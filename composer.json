{
    "name": "railken/composer-link",
    "keywords": ["laravel"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "doctrine/inflector": "^1.3",
        "eloquent/composer-config-reader": "^2.1",
        "symfony/console": "^4.2 || 5.*",
        "symfony/cache": "^4.2 || 5.*",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\ComposerLink\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Railken\\ComposerLink\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/composer-link"
    ]
}
